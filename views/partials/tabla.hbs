<table class="table">
        <thead>
            <tr>
                <th scope="col">Nombre de Producto</th>
                <th scope="col">Precio</th>
                <th scope="col">Imagen</th>
            </tr>
        </thead>
        <tbody>
            {{#each datos}}
                <tr>
                    <td class="td">{{producto}}</td>
                    <td class="td">  $ {{precio}}</td>
                    <td class="td">    <img src="{{imagen}}"alt="imagen producto"
                        width="200px" height="200px" class="img"></td>                    
                </tr>
                {{#if datos}}<td class="td">NO HAY PRODUCTOS</td>{{/if}}
            {{/each}}
        </tbody>
    </table>
